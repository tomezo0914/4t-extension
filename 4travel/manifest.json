{
  "name": "4T-Search",
  "version": "0.1",
  "manifest_version": 2,

  "description": "Search travel information in 4travel.",
  "icons": {
    "48": "img/icon48.png",
    "128": "img/icon128.png"
  },

  "browser_action": {
    "default_icon": "img/icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs"
  ],
  "content_security_policy": "script-src 'self'; object-src 'self'"
}
